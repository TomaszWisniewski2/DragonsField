{
  "version": 2,
  "builds": [
    {
      "src": "mtg-playtest/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build --prefix mtg-playtest",
  "outputDirectory": "mtg-playtest/dist"
}